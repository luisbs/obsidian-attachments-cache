{
  "plugin_level": "WARN",
  "plugin_priority": "NORMAL",
  "allow_characters": false,
  "url_param_cache": "cache_file",
  "url_param_ignore": "ignore_file",
  "note_param_cache": "cache_from",
  "note_param_ignore": "cache_unless",
  "note_param_rule": "cache_rule",
  "cache_rules": [
    {
      "id": "empty",
      "enabled": false,
      "storage": "attachments/{notename}",
      "pattern": "*",
      "remotes": []
    },
    {
      "id": "rule_override",
      "enabled": true,
      "storage": "attachments/{notename}",
      "pattern": "doesnt-match",
      "remotes": [
        {
          "pattern": "*",
          "whitelisted": true
        }
      ]
    },
    {
      "id": "default",
      "enabled": true,
      "storage": "attachments/{notepath}",
      "pattern": "*",
      "remotes": [
        {
          "pattern": "4kwallpapers.com/images",
          "whitelisted": false
        },
        {
          "pattern": "github.com/elementary/wallpapers",
          "whitelisted": true
        },
        {
          "pattern": "images.pexels.com/photos",
          "whitelisted": true
        },
        {
          "pattern": "*",
          "whitelisted": false
        }
      ]
    }
  ]
}